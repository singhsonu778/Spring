<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />

	<bean id="textEditor" class="com.sonu.spring.TextEditor">
		<property name="id" value="1" />
	</bean>
	<bean id="spellChecker" class="com.sonu.spring.SpellChecker" />
	<bean id="spellChecker1" class="com.sonu.spring.SpellChecker1" />
	<bean id="spellChecker2" class="com.sonu.spring.SpellChecker2" />
	<bean id="spellChecker3" class="com.sonu.spring.SpellChecker2" />

</beans>